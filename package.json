{
  "name": "fimweb-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npx -y serve@14.2.1 dist -p 3000",
    "local": "git checkout develop && git pull origin develop && next build && next start",
    "lint": "next lint",
    "knip": "knip --exclude enumMembers --no-exit-code",
    "depcruise": "depcruise src",
    "analyze:code": "npm run lint && npm run knip && npm run depcruise",
    "analyze:leaks": "npx -y fuite@3.0.0 --scenario .fuite/scenario.mjs --iterations 11",
    "analyze:leaks:gui": "npm run analyze:leaks -- --debug",
    "mock:api": "json-server --port 7071 --read-only --routes mocks/routes.json mocks/api.json",
    "format:check": "prettier --cache -c .",
    "format": "prettier --cache -w .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom=\"./src/**\""
  },
  "dependencies": {
    "@azure/msal-browser": "3.5.0",
    "@azure/msal-react": "2.0.7",
    "@contentful/rich-text-html-renderer": "16.3.0",
    "@contentful/rich-text-types": "16.3.0",
    "@epic-web/cachified": "4.0.0",
    "@fluentui/react": "8.112.9",
    "@fluentui/react-components": "9.42.0",
    "@fluentui/react-file-type-icons": "8.10.4",
    "@fluentui/react-hooks": "8.6.33",
    "@fluentui/react-icons": "2.0.223",
    "@fluentui/react-icons-mdl2-branded": "1.2.54",
    "@microsoft/microsoft-graph-client": "3.0.7",
    "@microsoft/microsoft-graph-types": "2.40.0",
    "classnames": "2.3.2",
    "dayjs": "1.11.10",
    "dompurify": "3.0.6",
    "file-saver": "2.0.5",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "lru-cache": "10.1.0",
    "micro-observables": "1.7.2",
    "mockdate": "^3.0.5",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.6",
    "react-use": "17.4.1",
    "sass": "1.69.5",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@jest/expect": "29.7.0",
    "@jest/globals": "29.7.0",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/dompurify": "3.0.5",
    "@types/file-saver": "2.0.7",
    "@types/jest": "29.5.10",
    "@types/lodash": "4.14.202",
    "@types/node": "18.18.10",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@types/uuid": "9.0.7",
    "dependency-cruiser": "15.5.0",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-sonarjs": "0.23.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "json-server": "0.17.4",
    "knip": "3.7.0",
    "prettier": "3.1.0",
    "typescript": "5.3.2"
  }
